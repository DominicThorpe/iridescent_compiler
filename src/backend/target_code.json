{
    "mips": {
        "start_func": [
            "{}: # start subroutine",
            "\tmove $fp, $sp",
            "\tsw $fp, 0($sp)",
            "\tsubi $sp, $sp, 4",
            "\tsubi $sp, $sp {}"
        ],

        "end_func": [
            "\tmove $sp, $fp  # end subroutine {}",
            "\tjr $ra",
            "\tnop"
        ],

        "end_main": [
            "\tj end # end of main"
        ],

        "push": {
            "int": [
                "\tli $t4, {} # push int",
                "\tsw $t4, 0($sp)",
                "\tsubi $sp, $sp, 4"
            ],

            "long": [
                "\tli $t4, {} # push long",
                "\tli $t5, {}",
                "\tsw $t4, 0($sp)",
                "\tsw $t5, -4($sp)",
                "\tsubi $sp, $sp, 8"
            ],

            "byte": [
                "\tli $t4, {} # push byte",
                "\tsb $t4, 0($sp)",
                "\tsubi $sp, $sp, 1"
            ]
        },

        "store" : {
            "int": [
                "\taddi $sp, $sp, 4 # store int",
                "\tlw $t0, 0($sp)",
                "\tsw $t0, -{}($fp)"
            ],

            "long": [
                "\taddi $sp, $sp, 8 # store long",
                "\tlw $t0, 0($sp)",
                "\tlw $t1, -4($sp)",
                "\tsw $t0, -{}($fp)",
                "\tsw $t1, -{}($fp)"
            ],

            "byte": [
                "\taddi $sp, $sp, 4 # store byte",
                "\tlb $t0, 0($sp)",
                "\tsb $t0, -{}($fp)"
            ]
        },

        "load": {
            "int": [
                "\tlw $t0, -{}($fp) # load int",
                "\tsw $t0, 0($sp)",
